{
    "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len": ["error", {"code": 120}],
        "semi": [2, "always"],
        "indent": ["error", 4]
    }
}
